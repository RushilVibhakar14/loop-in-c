#include<stdio.h>
void main()
{
    int i,j;
    printf("rushil vibhakar\n");
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i*2;j++)
        {
            if(j%2==1)
            {
                printf("0");
            }
            else
            {
                printf("1");
            }
        } printf("\n");
    }
}
